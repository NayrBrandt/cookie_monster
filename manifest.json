{
        "name": "Cookie Monster",
        "description": "Returns Sorted Cookie Details",
        "version": "1.2",
        "manifest_version": 3,
        "permissions": ["cookies", "tabs", "webNavigation", "webRequest", "scripting", "storage"],
        "host_permissions": ["<all_urls>"],
        "action": {
          "default_popup": "layout.html",
          "default_icon": "hello_extensions.png"
        },
        "background": {
          "service_worker": "cookies.js"
        }
}